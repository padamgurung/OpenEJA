package openeja.dp;

public class DBFactoryAdapter {
	
	public DBAdapter getAdapter(String adapterType) {
		
		if(adapterType == null) {
			return null;
		}
		
		try {
			return (DBAdapter)Class.forName(adapterType).newInstance();
		}
		catch (Throwable e) {
			System.out.println(e);
		}
		return null;
		
	}

}
