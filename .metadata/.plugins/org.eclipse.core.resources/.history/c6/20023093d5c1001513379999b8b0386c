package openeja.dp;

public class DBFactoryAdapter {
	
	public DBAdapter getAdapter(String adapterType) {
		
		
		if(adapterType.equalsIgnoreCase("MySQL")){
			return new MySQLAdapter();
		}
		else if(adapterType.equalsIgnoreCase("Oracle")){
			return new OracleAdapter();
		}
		else if(adapterType.equalsIgnoreCase("PostgreSQL")){
			return new PostgresSQLAdapter();
		}
		return null;
		
	}

}
