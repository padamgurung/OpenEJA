package OpenEJA.BO;

import OpenEJA.Annotation.*;
import OpenEJA.ORM.Entity;

@Table(name = "user")
public class User extends Entity {
		
	@Id @AutoGenerated
	@Column(name = "id", type = "Integer")
	public int id;
	
	@Column(name = "name")
	public String name;
	
	@Column(name = "email")
	public String email;
	
	@Column(name = "mobile_number")
	public String mobileNumber;
	
	public User(String name) {
		super(name);
		this.id = 0;
		this.name = "";
		this.email = "";
		this.mobileNumber = "";
	}
}
